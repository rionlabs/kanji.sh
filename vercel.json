{
  "framework": "remix",
  "installCommand": "npm install",
  "buildCommand": "npm run build:app",
  "ignoreCommand": "node tools/skipDeploy.cjs",
  "trailingSlash": false,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/write/collection/:key",
      "destination": "/write/:key",
      "permanent": true
    }
  ]
}
