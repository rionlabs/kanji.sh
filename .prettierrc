{
    "plugins": ["prettier-plugin-tailwindcss"],
    "semi": true,
    "printWidth": 100,
    "trailingComma": "none",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "overrides": [
        {
            "files": [
                "*.yaml",
                "*.yml"
            ],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
