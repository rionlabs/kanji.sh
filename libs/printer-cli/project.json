{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "printer-cli",
  "sourceRoot": "libs/printer-cli/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/printer-cli",
        "main": "libs/printer-cli/src/index.ts",
        "tsConfig": "libs/printer-cli/tsconfig.lib.json",
        "assets": ["libs/printer-cli/*.md"],
        "format": ["esm"],
        "esbuildOptions": {
          "alias": {
            "lodash": "lodash-es"
          }
        }
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run printer-cli:build --skip-nx-cache",
          "node dist/libs/printer-cli/index.js"
        ],
        "parallel": false,
        "env": {
          "DEBUG": "printer:cli"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/printer-cli/**/*.ts"]
      }
    }
  },
  "tags": []
}
